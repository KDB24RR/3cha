<mapper namespace="com.itcom.Chart.mapper.customerMapper">

    <select id="getCustomerById" parameterType="string" resultType="com.itcom.Chart.vo.CustomerVO">
        SELECT *
        FROM customer
        WHERE customer_id = #{customerId}
    </select>
</mapper>